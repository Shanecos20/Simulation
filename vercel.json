{
  "version": 2,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/Builds/index.html"
    }
  ],
  "headers": [
    {
      "source": "/Builds/Build/(.+\\.data\\.br)$",
      "headers": [
        { "key": "Content-Type",   "value": "application/octet-stream" },
        { "key": "Content-Encoding", "value": "br" }
      ]
    },
    {
      "source": "/Builds/Build/(.+\\.framework\\.js\\.br)$",
      "headers": [
        { "key": "Content-Type",   "value": "application/javascript" },
        { "key": "Content-Encoding", "value": "br" }
      ]
    },
    {
      "source": "/Builds/Build/(.+\\.wasm\\.br)$",
      "headers": [
        { "key": "Content-Type",   "value": "application/wasm" },
        { "key": "Content-Encoding", "value": "br" }
      ]
    }
  ]
}
