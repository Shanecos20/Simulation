{
  "version": 2,
  "routes": [
    {
      "src": "/Builds/Build/(.*\\.js)$",
      "headers": { "Content-Type": "application/javascript" },
      "continue": true
    },
    {
      "src": "/Builds/Build/(.*\\.data\\.br)$",
      "headers": { 
        "Content-Type": "application/octet-stream",
        "Content-Encoding": "br"
      },
      "continue": true
    },
    {
      "src": "/Builds/Build/(.*\\.wasm\\.br)$",
      "headers": { 
        "Content-Type": "application/wasm",
        "Content-Encoding": "br"
      },
      "continue": true
    },
    {
      "src": "/Builds/Build/(.*\\.framework\\.js\\.br)$",
      "headers": { 
        "Content-Type": "application/javascript",
        "Content-Encoding": "br"
      },
      "continue": true
    },
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/Builds/index.html" }
  ]
}
