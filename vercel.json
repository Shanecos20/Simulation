{
  "version": 2,
  "builds": [
    { 
      "src": "package.json", 
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/index.html",
      "dest": "/index.html"
    },
    {
      "src": "/Build/(.*)",
      "dest": "/Build/$1"
    },
    {
      "src": "/TemplateData/(.*)",
      "dest": "/TemplateData/$1"
    },
    {
      "src": "/StreamingAssets/(.*)",
      "dest": "/StreamingAssets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "headers": [
    {
      "source": "/Builds/Build/(.+\\.data\\.br)$",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/octet-stream"
        },
        {
          "key": "Content-Encoding",
          "value": "br"
        }
      ]
    },
    {
      "source": "/Builds/Build/(.+\\.framework\\.js\\.br)$",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript"
        },
        {
          "key": "Content-Encoding",
          "value": "br"
        }
      ]
    },
    {
      "source": "/Builds/Build/(.+\\.wasm\\.br)$",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/wasm"
        },
        {
          "key": "Content-Encoding",
          "value": "br"
        }
      ]
    }
  ]
}
